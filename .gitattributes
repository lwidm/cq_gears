# Track large files with Git LFS
docs/assets/*.png filter=lfs diff=lfs merge=lfs -text
docs/assets/*.jpg filter=lfs diff=lfs merge=lfs -text
docs/assets/*.jpeg filter=lfs diff=lfs merge=lfs -text
docs/assets/*.gif filter=lfs diff=lfs merge=lfs -text
docs/assets/*.mp4 filter=lfs diff=lfs merge=lfs -text
docs/assets/*.webm filter=lfs diff=lfs merge=lfs -text
docs/assets/*.svg filter=lfs diff=lfs merge=lfs -text
